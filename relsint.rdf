    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>
<rdf:RDF 
     xmlns:islandora="http://islandora.ca/ontology/relsint#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:dcterms="http://purl.org/dc/terms/">
   
  <rdf:Description rdf:about="http://islandora.ca/ontology/relsint#">
    <dcterms:title xml:lang="en">Islandora RELS-INT Ontology</dcterms:title>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2016-08-29</dcterms:modified>
    <dcterms:publisher rdf:resource="http://islandora.ca/"/>
    <rdfs:comment xml:lang="en">Ontology for Islandora RELS-INT, intended primarily to make it possible to expose Islandora-curated RDF predicates via de-reference-able URIs.</rdfs:comment>
    <owl:versionInfo>v1/2016/08/29</owl:versionInfo>
  </rdf:Description>
  
  <!-- http://islandora.ca/ontology/relsint#height_literal -->
  <rdf:Property rdf:about="http://islandora.ca/ontology/relsint#height_literal">
    <rdfs:label xml:lang="en">height literal</rdfs:label>
    <rdfs:comment xml:lang="en">The height of an image</rdfs:comment>
    <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    <skos:exactMatch rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#height" />
  </rdf:Property>
  
  <!-- http://islandora.ca/ontology/relsint#width_literal -->
  <rdf:Property rdf:about="http://islandora.ca/ontology/relsint#width_literal">
    <rdfs:label xml:lang="en">width</rdfs:label>
    <rdfs:comment xml:lang="en">The width of an image</rdfs:comment>
    <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    <skos:exactMatch rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#width" />
  </rdf:Property>

  <!-- http://islandora.ca/ontology/relsintt#isViewableByRole -->
  <rdf:Property rdf:about="http://islandora.ca/ontology/relsint#isViewableByRole">
    <rdfs:label xml:lang="en">is viewable by role</rdfs:label>
    <rdfs:range rdf:resource="&xsd;string"/>
  </rdf:Property>

  <!-- http://islandora.ca/ontology/relsint#isManageableByRole -->
  <rdf:Property rdf:about="http://islandora.ca/ontology/relsint#isManageableByRole">
    <rdfs:label xml:lang="en">is manageable by role</rdfs:label>
    <rdfs:range rdf:resource="&xsd;string"/>
  </rdf:Property>

  <!-- http://islandora.ca/ontology/relsint#isViewableByUser -->
  <rdf:Property rdf:about="http://islandora.ca/ontology/relsint#isViewableByUser">
    <rdfs:label xml:lang="en">is viewable by user</rdfs:label>
    <rdfs:range rdf:resource="&xsd;string"/>
  </rdf:Property>

  <!-- http://islandora.ca/ontology/relsint#isManageableByUser -->
  <rdf:Property rdf:about="http://islandora.ca/ontology/relsint#isManageableByUser">
    <rdfs:label xml:lang="en">is manageable by user</rdfs:label>
    <rdfs:range rdf:resource="&xsd;string"/>
  </rdf:Property>

